#!/bin/bash

TEST_OUT=test_out/test_faucetizer.out

export FORCH_CONFIG_DIR=topo/bond

if [ -z "$VIRTUAL_ENV" -a -f venv/bin/python3 ]; then
    PYTHON=venv/bin/python3
else
    PYTHON=python3
fi

mkdir -p test_out
rm -f $TEST_OUT

$PYTHON -m forch.faucetizer >> $TEST_OUT

diff etc/test_faucetizer.out $TEST_OUT || exit 1